<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<BorderPane fx:controller="org.branlewalk.view.ReportController"
            maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="typeColumn" text="Type"/>
                        <TableColumn fx:id="consultantColumn" text="Consultant"/>
                        <TableColumn fx:id="customerColumn" text="Customer"/>
                        <TableColumn fx:id="titleColumn" text="Title"/>
                        <TableColumn fx:id="startColumn" text="Start"/>
                        <TableColumn fx:id="endColumn" text="End"/>
                    </columns>
                    <StackPane.margin>
                        <Insets left="10.0" right="10.0"/>
                    </StackPane.margin>
                </TableView>
            </children>
        </StackPane>
    </center>
    <bottom>
        <HBox alignment="CENTER_RIGHT" prefHeight="40.0" prefWidth="600.0" BorderPane.alignment="CENTER_RIGHT">
            <children>
                <Button onAction="#handleBackButton" text="Button"/>
            </children>
        </HBox>
    </bottom>
    <top>
        <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="titleLabel" text="Label">
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
            </children>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
        </HBox>
    </top>
</BorderPane>
